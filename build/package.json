{
  "name": "@deno-hotwired/aws",
  "version": "0.0.1",
  "description": "Custom build for browser compatible AWS SDK distro",
  "private": true,
  "scripts": {
    "build:aws": "npx esbuild --bundle --minify --format=esm --platform=browser ./aws/*.js --outdir=../lib/aws/_build"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.42.0",
    "@aws-sdk/client-kinesis": "3.42.0",
    "@aws-sdk/client-s3": "3.42.0",
    "@aws-sdk/client-sesv2": "3.42.0",
    "@aws-sdk/client-sns": "3.42.0",
    "@aws-sdk/client-sqs": "3.42.0"
  }
}
